{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Debug Nest Framework",
            "args": [
                "${workspaceFolder}/src/main.ts"
            ],
            "sourceMaps": true,
            "envFile": "${workspaceFolder}/.env",
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
            "protocol": "inspector",
            "typescript.preferences.importModuleSpecifier": "relative"
        }
    ]
}